{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@shared/(.*)": "<rootDir>/../libs/@shared/src/$1",
    "@shared/": "<rootDir>/../libs/@shared/src"
  }
}